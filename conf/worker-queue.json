{
  "name": "jobs",
  "subjects": [
    "jobs.eu.*",
    "jobs.us.*",
    "jobs.results"
  ],
  "retention": "workqueue",
  "max_consumers": -1,
  "max_msgs_per_subject": -1,
  "max_msgs": 50000,
  "max_bytes": 1073741824,
  "max_age": 86400000000000,
  "max_msg_size": -1,
  "storage": "file",
  "discard": "new",
  "num_replicas": 1,
  "duplicate_window": 120000000000,
  "sealed": false,
  "deny_delete": false,
  "deny_purge": false,
  "allow_rollup_hdrs": false,
  "allow_direct": true,
  "mirror_direct": false,
  "consumer_limits": {}
}